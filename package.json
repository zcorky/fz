{
  "name": "@zcorky/fz",
  "version": "1.0.10",
  "description": "A simply fetch library",
  "main": "lib/index.js",
  "repository": "https://github.com/zcorky/fz",
  "author": "Zero",
  "license": "MIT",
  "scripts": {
    "dev": "tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf -rf lib",
    "test": "NODE_ENV=test nyc mocha --recursive test/**/*.spec.ts --exit --timeout 10000",
    "test:options": "NODE_ENV=test nyc mocha test/options.*.spec.ts --exit --timeout 10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.2",
    "create-test-server": "^2.4.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^4.0.2"
  },
  "files": [
    "lib"
  ],
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "lines": 85
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@zcorky/delay": "^1.0.1",
    "@zcorky/query-string": "^1.0.2",
    "isomorphic-fetch": "^2.2.1"
  }
}
